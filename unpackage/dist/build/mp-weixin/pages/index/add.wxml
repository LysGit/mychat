<view><view class="{{['my-bar',$config.bgClass]}}" style="{{(topBar.style)}}"><navigator class="action " style="width:15%;" open-type="navigateBack" data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}" bindtap="__e"><text class="cuIcon-back"></text><view>{{i18n.back}}</view></navigator><view class="content "><view class="text-cut text-center" style="width:220px;">{{i18n.add}}</view></view><view style="width:15%;"></view></view><view class="cu-bar bg-white search margin-bottom-sm"><view class="search-form"><text class="cuIcon-search"></text><input type="text" placeholder="{{i18n.pleaseInputUID}}" confirm-type="search"/></view><view data-event-opts="{{[['tap',[['search']]]]}}" class="action" bindtap="__e"><button class="cu-btn shadow-blur bg-blue">{{i18n.search}}</button></view></view><view class="cu-list menu-avatar"><block wx:for="{{search.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item contacts-list"><view class="cu-avatar round" style="{{'background-image:'+('url('+item.avatar+')')+';'}}"></view><view class="content"><view class="text-black text-xl">{{item.nickname}}</view></view><view class="action margin"><view class="bg-blue radius cu-btn">{{i18n.add}}</view></view></view></block></view></view>